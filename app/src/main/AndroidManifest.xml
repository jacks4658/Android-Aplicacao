<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ltf.aplicativo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.MainActivity"></activity>
        <activity android:name=".Activity.CadastroActivity" />
        <activity android:name=".Activity.LoginActivity"></activity>
        <activity android:name=".Activity.Main2Activity" />
        <activity android:name=".Activity.NewsActivity" />
        <activity android:name=".Activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.NewsWebView"
            android:label="@string/title_activity_news_web_view"
            android:parentActivityName=".Activity.NewsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ltf.aplicativo.Activity.NewsActivity" />
        </activity>
    </application>

</manifest>